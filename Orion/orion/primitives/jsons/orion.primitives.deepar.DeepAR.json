{
    "name": "orion.primitives.deepsr.DeepAR",
    "contributors": ["Abdullah Alomar <aalomar@mit.edu>"],
    "documentation": "",
    "description": "TAD using DeepAR.",
    "classifiers": {
        "type": "postprocessor",
        "subtype": "anomaly_detector"
    },
    "modalities": [],
    "primitive": "orion.primitives.deepar.DeepAR",
    "fit": {
        "method": "fit",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            }
        ]
    },
    "produce": {
        "method": "predict",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "y_hat",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "value_column": {
                "type": "str",
                "default": "value"
            },
            "time_column": {
                "type": "str",
                "default": "timestamp"
            },
            "freq": {
                "type": "str",
                "default": "1H"
            },
            "epochs": {
                "type": "int",
                "default": 20
            },
            "h": {
                "type": "int",
                "default": 1
            },
            "no_samples": {
                "type": "int",
                "default": 100
            }
        
        }
    }
}
